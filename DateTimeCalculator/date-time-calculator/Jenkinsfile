pipeline{
 agent any
 
 
 stages{
 	
 	stage ('Compile Stage') {
 		steps {
 		withMaven( maven : 'M3') {
 			sh 'mvn clean compile'
 		
 		}
 		
 		}
 	}
 	
 	stage ('Install Stage') {
 		steps {
 		withMaven( maven : 'M3') {
 			sh 'mvn install'
 		
 		}
 		
 		}
 	}
 	
 	stage ('Test Stage') {
 		steps {
 		withMaven( maven : 'M3') {
 			sh 'mvn test'
 		
 		}
 		
 		}
 	}
 
 }



}